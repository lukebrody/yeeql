{
	"name": "yeeql",
	"version": "1.0.7",
	"description": "A SQL-like interface around Y.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"/dist"
	],
	"license": "MIT",
	"scripts": {
		"test": "node --expose-gc node_modules/.bin/vitest",
